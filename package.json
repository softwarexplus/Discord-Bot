{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "A template for discord bot",
    "main": "out/src/index.js",
    "scripts": {
        "dev": "nodemon",
        "build": "tsc",
        "start": "node .",
        "test": "tsc"
    },
    "keywords": [
        "discord.js",
        "discord-bot"
    ],
    "author": "SoftwareX Plus",
    "license": "MIT",
    "dependencies": {
        "coolcake": "^0.0.2",
        "discord.js": "^14.14.1"
    },
    "devDependencies": {
        "@types/node": "^20.9.3",
        "typescript": "^5.3.2",
        "nodemon": "^3.0.1",
        "tsx": "^3.14.0"
    },
    "prettier": {
        "trailingComma": "none",
        "semi": false,
        "singleQuote": false,
        "tabWidth": 4,
        "printWidth": 120
    },
    "nodemonConfig": {
        "ext": ".ts, .json",
        "exec": "tsx src/index.ts",
        "watch": [
            "src"
        ]
    }
}
